{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"sourceMap": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"suppressImplicitAnyIndexErrors": true
	}
}
